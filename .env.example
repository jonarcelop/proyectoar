# .env.example
# Copy this to .env and fill in your values

# Leaderboard Backend
# If not set, uses JSON file (leaderboard.json)
# If set, uses PostgreSQL via SQLAlchemy
# DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/triqui

# FastAPI/Uvicorn (optional)
# SERVER_HOST=127.0.0.1
# SERVER_PORT=8000
# DEBUG=True

# CORS Origins (optional, default is "*" for development)
# CORS_ORIGINS=["http://127.0.0.1:8080","http://localhost:3000"]
